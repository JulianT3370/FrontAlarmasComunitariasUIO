{
  "expo": {
    "name": "FrontAlarmaComunitariaUIO",
    "slug": "frontalarma",
    "version": "1.0.0",
    "orientation": "portrait",
    "platforms": ["ios", "android", "web"],
    "newArchEnabled": true,
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "build": {
        "output": "web-build"
      }
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.frontalarma.app"
    },
    "assetBundlePatterns": [
      "**/*"
    ]
  }
}
